{
  "name": "jetpack",
  "version": "4.4.1",
  "main": "index.js",
  "description": "Jetpack wraps webpack and nodemon to give you the best development workflow.",
  "license": "MIT",
  "author": "Karolis Narkevicius",
  "repository": {
    "type": "git",
    "url": "git://github.com/KidkArolis/jetpack.git"
  },
  "keywords": [
    "webpack",
    "jetpack",
    "build"
  ],
  "engines": {
    "node": ">=20"
  },
  "bin": {
    "jetpack": "./bin/jetpack"
  },
  "scripts": {
    "test": "ava && eslint . && prettier --check '**/*.{js,json,css,yml}'",
    "format": "prettier --write '**/*.{js,json,css,yml}'",
    "release": "np",
    "release-alpha": "np --tag=alpha --any-branch --no-cleanup"
  },
  "dependencies": {
    "@rspack/core": "^1.7.4",
    "@rspack/plugin-react-refresh": "^1.6.0",
    "@swc/core": "^1.15.11",
    "@swc/helpers": "^0.5.18",
    "browserslist": "^4.28.1",
    "browserslist-useragent-regexp": "^4.1.3",
    "core-js": "^3.48.0",
    "css-loader": "^7.1.3",
    "express": "^5.2.1",
    "handlebars": "^4.7.8",
    "parseurl": "^1.3.3",
    "picocolors": "^1.1.1",
    "prepend-transform": "0.0.1019",
    "react-refresh": "^0.18.0",
    "regenerator-runtime": "^0.14.1",
    "sass-embedded": "^1.97.3",
    "sass-loader": "^16.0.6",
    "sass-resources-loader": "^2.2.5",
    "send": "^1.2.1",
    "style-loader": "^4.0.0",
    "webpack-bundle-analyzer": "^5.2.0",
    "webpack-dev-middleware": "^7.4.5",
    "webpack-format-messages": "^3.0.1",
    "webpack-hot-middleware": "^2.26.1"
  },
  "devDependencies": {
    "ava": "^6.4.1",
    "eslint": "^9.39.2",
    "execa": "^9.6.1",
    "klaw": "^4.1.0",
    "neostandard": "^0.12.2",
    "prettier": "^3.8.1",
    "string-replace-loader": "^3.3.0"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "timeout": "1m"
  }
}
