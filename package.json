{
  "name": "jetpack",
  "version": "4.2.0",
  "main": "index.js",
  "description": "Jetpack wraps webpack and nodemon to give you the best development workflow.",
  "license": "MIT",
  "author": "Karolis Narkevicius",
  "repository": {
    "type": "git",
    "url": "git://github.com/KidkArolis/jetpack.git"
  },
  "keywords": [
    "webpack",
    "jetpack",
    "build"
  ],
  "engines": {
    "node": ">=20"
  },
  "bin": {
    "jetpack": "./bin/jetpack"
  },
  "scripts": {
    "test": "ava && healthier && prettier --check '**/*.{js,json,css,yml}'",
    "format": "prettier --write '**/*.{js,json,css,yml}'",
    "release": "np",
    "release-alpha": "np --tag=alpha --any-branch --no-cleanup"
  },
  "dependencies": {
    "@rspack/core": "^1.5.0",
    "@rspack/plugin-react-refresh": "^1.4.3",
    "@swc/core": "^1.13.5",
    "@swc/helpers": "^0.5.17",
    "browserslist": "^4.25.3",
    "browserslist-useragent-regexp": "^4.1.3",
    "core-js": "^3.45.1",
    "css-loader": "^7.1.2",
    "express": "^5.1.0",
    "handlebars": "^4.7.8",
    "parseurl": "^1.3.3",
    "picocolors": "^1.1.1",
    "prepend-transform": "0.0.1019",
    "react-refresh": "^0.17.0",
    "regenerator-runtime": "^0.14.1",
    "sass-embedded": "^1.91.0",
    "sass-loader": "^16.0.5",
    "sass-resources-loader": "^2.2.5",
    "send": "^1.2.0",
    "style-loader": "^4.0.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-dev-middleware": "^7.4.2",
    "webpack-format-messages": "^3.0.1",
    "webpack-hot-middleware": "^2.26.1"
  },
  "devDependencies": {
    "ava": "^6.4.1",
    "execa": "^9.6.0",
    "healthier": "^7.0.0",
    "klaw": "^4.1.0",
    "prettier": "^3.6.2",
    "string-replace-loader": "^3.2.0"
  },
  "healthier": {
    "ignore": [
      "examples",
      "test/fixtures/pkg-with-everything"
    ],
    "global": [
      "describe",
      "it",
      "afterEach",
      "beforeEach"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "timeout": "1m"
  }
}
