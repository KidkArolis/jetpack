{
  "name": "jetpack",
  "version": "3.0.0",
  "main": "index.js",
  "description": "Jetpack wraps webpack and nodemon to give you the best development workflow.",
  "license": "MIT",
  "author": "Karolis Narkevicius",
  "repository": {
    "type": "git",
    "url": "git://github.com/KidkArolis/jetpack.git"
  },
  "keywords": [
    "webpack",
    "jetpack",
    "build"
  ],
  "engines": {
    "node": ">=16"
  },
  "bin": {
    "jetpack": "./bin/jetpack"
  },
  "scripts": {
    "test": "ava && healthier && prettier --check '**/*.{js,json,css,yml}'",
    "format": "prettier --write '**/*.{js,json,css,yml}'",
    "release": "np",
    "release-alpha": "np --tag=alpha --any-branch --no-cleanup"
  },
  "dependencies": {
    "@rspack/core": "^1.0.14",
    "@rspack/plugin-react-refresh": "^1.0.0",
    "@swc/core": "^1.6.13",
    "browserslist": "^4.23.1",
    "browserslist-useragent-regexp": "^4.1.3",
    "commander": "^12.1.0",
    "core-js": "^3.37.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "inquirer-confirm": "^2.0.7",
    "parseurl": "^1.3.3",
    "picocolors": "^1.0.1",
    "postcss": "^8.4.39",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^16.1.0",
    "postcss-load-config": "^6.0.1",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.6.0",
    "prepend-transform": "0.0.1019",
    "react-refresh": "^0.14.2",
    "regenerator-runtime": "^0.14.1",
    "require-relative": "^0.8.7",
    "sass-loader": "^14.2.1",
    "sass-resources-loader": "^2.2.5",
    "send": "^0.18.0",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.10",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-dev-middleware": "^7.2.1",
    "webpack-format-messages": "^3.0.1",
    "webpack-hot-middleware": "^2.26.1"
  },
  "devDependencies": {
    "ava": "^6.1.3",
    "execa": "^9.3.0",
    "healthier": "^7.0.0",
    "klaw": "^4.1.0",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "string-replace-loader": "^3.1.0"
  },
  "healthier": {
    "ignore": [
      "examples",
      "test/fixtures/pkg-with-everything"
    ],
    "global": [
      "describe",
      "it",
      "afterEach",
      "beforeEach"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "timeout": "1m"
  }
}
